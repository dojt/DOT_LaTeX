@article{MondSmithVanstraten03,
  author =       "Mond, David and Smith, Jim Q. and van Straten, Duco",
  title =        "{Stochastic factorizations, sandwiched simplices and
                  the topology of the space of explanations.}",
  language =     "English",
  journal =      "Proc. R. Soc. Lond., Ser. A, Math. Phys. Eng. Sci.",
  volume =       459,
  number =       2039,
  pages =        "2821-2845",
  year =         2003,
  doi =          {10.1098/rspa.2003.1150},
  abstract =     "{This paper is justified by a statistical question,
                  but uses Morse theory and singularity theory to
                  progress towards its solution. In \S\,2 the
                  statistical question is translated into a question
                  in combinatorial geometry which the authors attack
                  in \S\S\,3 and 4 using a version of Morse theory for
                  a certain class of piecewise smooth functions. These
                  ideas form the technical heart of this paper, and
                  might be read with interest for their geometrical
                  contact independently of any application to
                  statistics. In \S\,1 the authors show the folwing
                  theorems: Theorem 1.1. Let $V$ be an $n\times m$
                  stochastic matrix of rank $r$. If $V$ is ``small'',
                  then the space of rank $n$ size stochastic
                  factorizations of $V$ is homotopy-equivalent to the
                  space of $(r-1)$ simplices with vertices on the
                  $(r-2)$-spheres $S^{r-2}$.  Theorem 1.2. Let $V$ be
                  an $n\times m$ stochastic matrix of rank 3. The
                  quotient by the action of the symmetric $S_3$
                  (permuting the columns of $U$) of the space of
                  factorizations $V= UW$, with $U$ and $W$ stochastic
                  matrices of size $n\times 3$ and $3\times m$,
                  respectively, may be empty, and otherwise is
                  homotopy-equivalent to a circle, or has $k$
                  contractible components, where $0\le k\le m+
                  n$. When $m= n$, there are stochastic matrices $V$
                  for which this upper bound is realized.  \S\,2 is
                  entitled ``Stochastic factorizations and
                  cones''. The central of this paragraph is Lemma
                  2.2. Suppose that $V$ is an $n\times m$ stochastic
                  matrix of rank $r$. If (i) the vectors $\widehat
                  u_1,\dots,\widehat u_r$ are in $L(V)\cap \bbfR^m_+$,
                  and (ii) the cone $C(\widehat u_1,\dots,\widehat
                  u_r)$ contains the cone $C(V)$, then (a) there exist
                  unique coefficients $\alpha_i> 0$ such that $\sum_i
                  \alpha_i u_i= 1$, and (b) denoting the vectors
                  $\alpha_i\widehat u$ by $u_i$ there exist unique
                  $\beta_{i,j}\ge 0$ such that $v_j= \sum_i
                  \beta_{i,j}u_i$ for each $i$, and such that $\sum_j
                  \beta_{i,j}= 1$ for each $j$.  That is, $V$
                  factorizes as the product of the stochastic matrix
                  $U$ with columns $u_1,\dots, u_r$ and stochastic
                  matrix $B= [\beta_{i,j}]$. This paragraph contains
                  also Theorem 2.4. Let $V$ be an $n\times m$
                  stochastic matrix of rank $r$, then $SF_r(V)$ is
                  diffeomorphic to $\overline\Delta v_i w_j$.  In
                  \S\,3 ``Topology of the space of explanations
                  $\Delta v_s w_s$'' for the space $SF_r(V)$ of
                  rank-size stochastic factorizations of a given
                  $n\times m$ stochastic matrix modulo aliasing,
                  information is obtained about the homotopy-type of
                  $SF_r(V)$.  Theorem 3.8. Let $f_1,\dots, f_k$ be
                  smooth functions in a general position on the smooth
                  manifold $M^n$, $f= \min\{f_1,\dots, f_k\}$, and let
                  $x\in M$ be a point at which the values of all $k$
                  functions coincide. Then either $x$ is a
                  topologically non-critical point of $f$, or the germ
                  of $f$ at $x$ is topologically equivalent to an
                  ordinary Morse critical point of index $\ge k-1$.
                  \S\,4 treats the 3-factorization of stochastic
                  matrices of rank 3.}",
  reviewer =     "{G. G. Vr\u anceanu (Bucure\c{s}ti)}",
  keywords =     "{stochastic matrices; hidden variables; topology;
                  Morse theory}",
  classmath =    "{60J10 (Markov chains (discrete-time Markov
                  processes on discrete state spaces)) 57R70 (Critical
                  points and critical submanifolds) 60A10
                  (Probabilistic measure theory) }",
}

